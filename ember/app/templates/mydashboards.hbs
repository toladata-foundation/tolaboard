<div class="container">
	<h3>TolaBoards</h3>
</div>

<div class="container">
	<div class="row">
		<h4>My TolaBoards</h4>
	</div>
	<div class="row">
		{{#each model as |board|}}
			<div class="col-md-4">
				<a class="thumbnail data-internal" href="#">
					<div class="datasource-item">{{board.title}}</div>
				    <p class="datasource-date">{{board.createDate}}</p>
				</a>			
			</div>
		{{/each}}		
	</div> 	
</div>
